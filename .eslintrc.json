{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      }
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": ["error", "windows"],
      "import/extensions": 0,
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0,
      "no-useless-constructor": 0
    }
}
